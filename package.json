{
  "name": "home-library",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && nest build -p tsconfig.typeorm.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production npm run typeorm:migrate && node dist/app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testPathIgnorePatterns refresh.e2e.spec.ts --noStackTrace --runInBand",
    "test:auth": "cross-env TEST_MODE=auth jest --testPathIgnorePatterns refresh.e2e.spec.ts --noStackTrace --runInBand",
    "test:refresh": "cross-env TEST_MODE=auth jest --testPathPattern refresh.e2e.spec.ts --noStackTrace --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm:dev": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d typeorm/source.ts",
    "typeorm:prod": "node node_modules/typeorm/cli.js -d dist/typeorm/source.js",
    "typeorm:migrate": "npm run typeorm:prod -- migration:run",
    "typeorm:migration:generate": "npm run typeorm:dev -- migration:generate",
    "docker:scout": "docker scout cves ant0x64/nodejs-service_db:latest && docker scout cves ant0x64/nodejs-service_app:latest"
  },
  "dependencies": {
    "@nestjs/common": ">=8.0.0",
    "@nestjs/core": ">=8.0.0",
    "@nestjs/platform-express": ">=8.0.0",
    "@nestjs/jwt": ">=9.0.0",
    "@nestjs/mapped-types": ">=1.0.1",
    "@nestjs/swagger": ">=5.2.1",
    "@nestjs/typeorm": ">=10.0.2",
    "bcrypt": ">=5.0.1",
    "class-transformer": ">=0.5.1",
    "class-validator": ">=0.13.2",
    "dotenv": ">=16.0.1",
    "reflect-metadata": ">=0.1.13",
    "rimraf": ">=3.0.2",
    "rxjs": ">=7.2.0",
    "swagger-ui-express": ">=5.0.0",
    "typeorm": ">=0.3.20",
    "pg": ">=8.11.3"
  },
  "devDependencies": {
    "@nestjs/cli": ">=8.0.0",
    "@nestjs/schematics": ">=8.0.0",
    "@nestjs/testing": ">=8.0.0",
    "@types/bcrypt": ">=5.0.0",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": ">=8.5.8",
    "@types/node": ">=16.0.0",
    "@types/supertest": ">=2.0.11",
    "@types/uuid": ">=8.3.4",
    "@typescript-eslint/eslint-plugin": ">=5.0.0",
    "@typescript-eslint/parser": ">=5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": ">=8.3.0",
    "eslint-plugin-prettier": ">=4.0.0",
    "jest": ">=27.2.5",
    "http-status-codes": ">=2.2.0",
    "prettier": ">=2.3.2",
    "prettier-eslint": "16.3.0",
    "source-map-support": "^0.5.20",
    "supertest": ">=6.2.3",
    "ts-jest": ">=27.0.3",
    "ts-loader": ">=9.5.1",
    "ts-node": ">=10.9.2",
    "tsconfig-paths": ">=3.15.0",
    "typescript": ">=4.3.5"
  }
}
